{
  "name": "@leadoo/react-bot",
  "version": "0.1.0",
  "description": "React components for Leadoo bots",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "run-s clean build:web",
    "build:web": "webpack --mode production --config webpack.config.js",
    "clean": "rimraf ./dist",
    "format": "prettier --write '{source,stories}/**/*.{ts,tsx,js,jsx}'",
    "prepublishOnly": "npm run build",
    "storybook": "NODE_PATH=source start-storybook",
    "test": "npm run test:format",
    "test:format": "prettier-check '{source,stories}/**/*.{ts,tsx,js,jsx}'"
  },
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "lint-staged": {
    "{source,stories}/**/*.{ts,tsx,js,jsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeadooMT/react-bot.git"
  },
  "keywords": [
    "leadoo",
    "react",
    "bot",
    "chat"
  ],
  "author": "Perry Mitchell <perry@leadoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LeadooMT/react-bot/issues"
  },
  "homepage": "https://github.com/LeadooMT/react-bot#readme",
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@storybook/react": "^5.3.19",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prettier-check": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
